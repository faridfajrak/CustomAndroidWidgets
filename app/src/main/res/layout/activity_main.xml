<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.f.fajrak.widget.indicatorimagebutton.IndicatorImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:icon_disable="@drawable/shop_inactive_btn"
        app:icon_enable="@drawable/shop_active"
        app:indicator_text_size="4sp"
        app:text_box_alignment="text_box_bottom"
        app:indicator_text_font_family="@font/roboto_reg"
        android:text="تست ماژول"
        app:isComplete="true"
        android:enabled="false"
        app:indicator_background="@drawable/btn_ripple_round"
        app:indicator_icon="@drawable/check_ic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>